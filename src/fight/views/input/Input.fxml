<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="768.0" prefWidth="1024.0"
	xmlns:fx="http://javafx.com/fxml/1"
	xmlns="http://javafx.com/javafx/2.2"
	fx:controller="fight.views.input.Input">
	<children>
		<ScrollPane layoutX="0.0" layoutY="0.0" prefHeight="768.0"
			prefWidth="1024.0">
			<content>
				<SplitPane dividerPositions="0.5" focusTraversable="true"
					prefHeight="768.0" prefWidth="1024.0">
					<items>
						<BorderPane fx:id="INPUT_ATTACKER" layoutX="0.0"
							layoutY="0.0" prefHeight="200.0" prefWidth="200.0" />
						<BorderPane fx:id="INPUT_DEFENDER" layoutX="0.0"
							layoutY="0.0" prefHeight="200.0" prefWidth="200.0" />

					</items>
				</SplitPane>
			</content>
		</ScrollPane>
	</children>
</AnchorPane>
